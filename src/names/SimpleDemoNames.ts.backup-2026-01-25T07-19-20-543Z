/**
 * Simple Demo Component Names
 * 
 * Stateless demo using HOC pattern with callbacks.
 * No StateManager, no localStorage persistence.
 */

export const SimpleDemoNames = {
  // Container
  container: 'simple-demo-container',
  
  // Menu buttons
  openMenu: 'simple-open-menu',
  closeMenu: 'simple-close-menu',
  
  // Toggle checkboxes
  featureToggle: 'simple-feature-toggle',
  notificationsToggle: 'simple-notifications-toggle',
  
  // Priority radio buttons
  priorityHigh: 'simple-priority-high',
  priorityMedium: 'simple-priority-medium',
  priorityLow: 'simple-priority-low',
  
  // Select dropdowns
  statusDropdown: 'simple-status-dropdown',
  themeSelect: 'simple-theme-select',
  
  // Form inputs
  formName: 'simple-form-name',
  formSubmit: 'simple-form-submit',
} as const;

export type SimpleDemoNameId = typeof SimpleDemoNames[keyof typeof SimpleDemoNames];

