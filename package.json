{  
  "name": "@supernal/monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Supernal Interface Monorepo",
  "workspaces": [
    "open-source",
    "enterprise",
    "docs-site"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "next": "^15.5.9",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "copilotkit": "^0.0.58"
  }
}
