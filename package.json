{
  "name": "@supernal-interface/docs-site",
  "version": "1.0.0",
  "description": "Documentation site and working demo for @supernal/interface",
  "private": true,
  "overrides": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "sync:docs": "tsx scripts/sync-docs.ts",
    "validate": "tsx scripts/validate-client-directives.ts",
    "prebuild": "(npm run validate || true) && (npm run sync:docs || true)",
    "build": "next build",
    "start": "PORT=$(node scripts/find-port.js) && next start -p $PORT",
    "export": "next build && next export",
    "generate": "supernal-interface generate",
    "deploy": "npm run build && npm run export",
    "pretest": "npm run story:generate || true",
    "test": "playwright test --reporter=list",
    "test:pre-build": "playwright test tests/pre-build.spec.ts --reporter=list",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:watch": "playwright test --reporter=list --watch",
    "test:nav": "playwright test tests/e2e/auto-navigation.spec.ts --reporter=list",
    "test:nav:watch": "playwright test tests/e2e/auto-navigation.spec.ts --reporter=list --watch",
    "test:blog": "playwright test tests/e2e/blog-navigation.spec.ts --reporter=list --headed",
    "test:tools": "node scripts/test-tools.js",
    "test:integration": "npm run build && playwright test tests/e2e/widget-tool-integration.spec.ts",
    "contracts:routes": "si scan-routes --pages src/pages --output src/architecture/Routes.ts",
    "contracts:names": "si scan-names --components src/components --output src/architecture/Components.ts",
    "contracts:all": "npm run contracts:routes && npm run contracts:names",
    "contracts:watch": "si scan-routes --watch & si scan-names --watch",
    "si:generate-tests": "si generate-tests --output tests/generated --routes src/architecture/Routes.ts --names src/architecture/Components.ts --include-e2e",
    "story:generate": "node ../enterprise/dist/cjs/cli/index.js generate-story-tests features -o tests/generated/stories",
    "story:watch": "chokidar 'features/**/*.feature' -c 'npm run story:generate'",
    "build:tests": "node ../enterprise/dist/cjs/cli/index.js generate-story-tests features -o tests/generated/stories --check --name-contracts src/architecture/DemoComponentNames.ts --data-contracts src/architecture/DemoComponentData.ts",
    "build:tests:force": "node ../enterprise/dist/cjs/cli/index.js generate-story-tests features -o tests/generated/stories",
    "pretest:stories": "npm run story:generate",
    "test:stories": "npm run pretest:stories && playwright test tests/generated/stories --reporter=list",
    "si:benchmark": "si benchmark-cache --runs 5 --output benchmark-results.json",
    "si:validate": "si validate --routes src/architecture/Routes.ts --names src/architecture/Components.ts --tools",
    "mcp": "node mcp-server.js",
    "mcp:debug": "node mcp-server.js 2>&1 | tee mcp-debug.log"
  },
  "dependencies": {
    "@copilotkit/react-core": "^1.50.1",
    "@copilotkit/react-textarea": "^1.50.1",
    "@copilotkit/react-ui": "^1.50.1",
    "@copilotkit/runtime": "^1.10.6",
    "@supernal/interface": "file:../open-source",
    "@supernalintelligence/interface-enterprise": "^1.0.14",
    "@types/geopattern": "^1.2.9",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xyflow/react": "^12.10.0",
    "d3-dag": "^1.1.0",
    "d3-hierarchy": "^3.1.2",
    "elkjs": "^0.11.0",
    "framer-motion": "^12.23.26",
    "geopattern": "^1.2.3",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.27",
    "lucide-react": "^0.561.0",
    "next": "^15.5.9",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "supernal-tts-widget": "^1.1.1",
    "typescript": "^5.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.5.9",
    "eslint-plugin-react": "^7.33.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0"
  }
}
